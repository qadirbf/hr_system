<h1 class="title"><%= @title %></h1>
<table class="list_tb">
  <tr class="title">
    <td>项目ID</td>
    <td>公司</td>
    <td>对象</td>
    <td>职位</td>
    <td>电话</td>
    <td>面试日期</td>
  </tr>
  <% @dailies.each do |d| %>
      <tr>
        <td><%= open_demand_page d.demand_id, d.demand_id %></td>
        <td>
          <%= d.show_firm_name -%>
        </td>
        <td><%= d.show_contact_name %></td>
        <td><%= d.position_cn %></td>
        <td><%= d.phone %></td>
        <td>
          <%= d.app_interview_date -%>
        </td>
      </tr>
  <% end %>
</table>